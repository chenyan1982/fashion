{
  "name": "smart-store",
  "version": "1.0.0",
  "description": "smart-store",
  "main": "src/server",
  "engines": {
    "node": "6.2.2",
    "npm": "3.7.3"
  },
  "scripts": {
    "test": "test",
    "clean": "rm -rf $OUTPUT_DIR",
    "lint": "eslint --fix --ext js --ext jsx src",
    "prod-build-client": "webpack --config ./webpack/config.js",
    "prod-build-server": "babel ./src -d $OUTPUT_DIR/server --source-maps",
    "prod-build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build ; fi",
    "start": "node $OUTPUT_DIR/server/server",
    "predev-start": "npm run clean",
    "dev-start": "nodemon -e js,jsx,md,mustache -w ./src --exec ./dev-tools/babel-run -- ./src/server",
    "dev": "nf start -w --procfile ./dev-tools/Procfile.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenyan1982/smart-store.git"
  },
  "keywords": [
    "smart-store",
    "sms",
    "texting",
    "campaigns",
    "campaigning",
    "politics",
    "activism",
    "organizing",
    "field"
  ],
  "author": "Calvin Chen",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/chenyan1982/smart-store/issues"
  },
  "homepage": "https://github.com/chenyan1982/smart-store/#readme",
  "dependencies": {
    "aphrodite": "^0.4.1",
    "apollo-client": "^0.3.29",
    "apollo-server": "^0.1.5",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "graphql": "^0.6.1",
    "graphql-tag": "^0.1.9",
    "minilog": "^3.0.1",
    "react": "^15.2.1",
    "react-apollo": "^0.3.15",
    "react-dom": "^15.2.1",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux-thunk": "^2.1.0",
    "rollbar": "^0.6.2",
    "thinky": "^2.3.3",
    "webpack": "^1.13.1",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "https://registry.npmjs.org/babel-cli/-/babel-cli-6.10.1.tgz",
    "babel-eslint": "^6.1.2",
    "babel-register": "^6.9.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "foreman": "^2.0.0",
    "nodemon": "^1.9.2",
    "webpack-dev-server": "^1.14.1"
  }
}
